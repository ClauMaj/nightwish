<!DOCTYPE html>
<html>

<head>
    <title><%= title %> </title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
        <link rel="stylesheet" href="/css/style.css">
        <link href="css/icofont/icofont.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans|Roboto+Slab' rel='stylesheet' type='text/css'>
<link href="https://fonts.googleapis.com/css2?family=Yeon+Sung&display=swap" rel="stylesheet">

</head>

<body>
  


  <div id='chatDiv'>
    <div id='chatFormDiv'>
      <div id='minim'>
        <p id="minimBox">-</p>
      </div>
      <div id="allMessages">
        <ul id="messages">

        </ul>
      </div>
      <form id="write">
        <input type="text" id="chat">
        <input type="submit" id="submitText" value="Send">
      </form>
    </div>
  </div>

    <div class="container-fluid px-0">
      
        <div class="headimg">
            <div id="transp"></div>
        </div>
        <nav  class="navbar navbar-expand-lg  bg-dark p-0">
          <!-- nav mobile button -->
            <a  class="navbar-brand ml-5" href="/"><img src="/images/logo.png" alt=""></a>
            <button type="button" class="mobile-nav-toggle "><i class="icofont-navigation-menu"></i></button>
      
            <div class="collapse navbar-collapse d-flex align-items-right" id="mainNav">
              <ul class="navbar-nav ml-auto mr-5">
                <li class="nav-item mr-3">
                  <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
                </li>
            </li>
            <li class="nav-item mr-3">
              <a class="nav-link" href="/about">About</a>
            </li>
            <li class="nav-item dropdown mr-3">
                  <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    Albums
                  </a>
                  <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                    <a class="dropdown-item" href="/albums">All albums</a>
                    <div class="dropdown-divider"></div>
                    
                  <% allAlbums.forEach(el => { %>
                    <a class="dropdown-item" href="/albums/<%= el.shortname %>"><%= el.albumname %> </a>
                  <% }) %>
                  </div>
                </li>
                <li class="nav-item mr-3">
                  <a class="nav-link" href="/feedback" >Feedback</a>
                </li>
                <li class="nav-item mr-3">
                  <a class="nav-link" href="https://nightwish.com/home#tour" target="_blank">Concerts</a>
                </li>
              </ul>
              
            </div>
          </nav>
        <!-- cut from here -->
    <div class="row pb-5 pt-0 mx-0 px-0">
        <div class="col-12">
    <hr class="style-one">
    </div>
    </div>